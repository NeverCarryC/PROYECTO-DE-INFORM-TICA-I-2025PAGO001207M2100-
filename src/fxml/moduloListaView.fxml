<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ModuloController">
   <children>
      <HBox alignment="CENTER" prefHeight="26.0" prefWidth="247.0">
         <children>
            <Button fx:id="backBtn" mnemonicParsing="false" onAction="#back" text="&lt;" />
            <Region prefHeight="26.0" prefWidth="121.0" HBox.hgrow="ALWAYS" />
            <Button fx:id="colapsarBtn" mnemonicParsing="false" onAction="#colapsar" text="Colapsar Todo" />
         </children>
      </HBox>
   		<TreeView fx:id="courseTreeView" VBox.vgrow="ALWAYS" />
   </children>
</VBox>
